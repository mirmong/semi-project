package semi.service.enums;

public enum OrderWay {

	DESC (1, "높은순", " desc"),
	ASC  (2, "낮은순", " asc");
	
	public final int no;
	public final String label;
	public final String sql;
	
	private OrderWay(int no, String label, String sql) {
		this.no = no;
		this.label = label;
		this.sql = sql;
	}
	
}