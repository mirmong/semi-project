					<script>
						(function(i, s, o, g, r, a, m, board_no) {
							if (s.getElementById(g)) {
								return
							}
							;
							m = new RegExp("[\?&]board_no=([^&#]*)")
									.exec(location.search),
									board_no = m ? decodeURIComponent(m[1]
											.replace(/\+/g, " ")) : '';
							if (board_no == "4") {
								document
										.write("<style>.crema-hide { display: none; }</style>");
								document
										.write("<div style='margin: 0 auto; text-align: center;'><img src='/web/upload/madeleine/photoqueen.jpg'></div>");
